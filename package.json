{
  "name": "dashforge-ui",
  "version": "0.1.40",
  "private": false,
  "description": "Dashboard-focused Vue 3 UI framework skeleton (TypeScript, Vite, SCSS, theming)",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:lib": "vite build",
    "play": "vite --config playground/vite.config.ts",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest --run",
    "gen:docs": "node scripts/generate-docs.mjs",
    "gen:utils": "node scripts/generate-utility-classes.mjs"
  },
  "keywords": [
    "vue",
    "ui",
    "dashboard",
    "theme",
    "vite",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^28.1.0",
    "sass": "^1.97.3",
    "typescript": "^5.5.4",
    "vite": "^7.3.1",
    "vitest": "^1.3.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "type": "module"
}
